{
  "name": "auth-agent",
  "version": "0.0.4",
  "description": "Creates NEAR wallets on testnet, verifies wallet connection, and checks token ownership on 1000fans.testnet.",
  "category": "agent",
  "tags": ["auth", "near", "token", "wallet", "access"],
  "details": {
    "agent": {
      "defaults": {
        "model": "llama-v3p1-70b-instruct",
        "model_provider": "fireworks",
        "model_temperature": 1.0,
        "model_max_tokens": 16384
      },
      "env_vars": {
        "CONTRACT_ID": "devbot.near",
        "NFT_CONTRACT_ID": "1000fans.testnet",
        "GROUP_ID": "theosis",
        "ADMIN_ACCOUNT_ID": "1000fans.testnet",
        "ADMIN_PRIVATE_KEY": ""
      }
    }
  },
  "show_entry": true
}