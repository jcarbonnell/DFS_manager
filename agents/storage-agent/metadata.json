{
  "name": "storage-agent",
  "version": "0.0.8",
  "description": "A real-world agent that encrypts an embedded .mp3 file, uploads to IPFS and stores CIDs on the NEAR blockchain.",
  "category": "agent",
  "tags": ["storage", "ipfs", "near", "encryption", "mp3"],
  "details": {
    "agent": {
      "defaults": {
        "model": "llama-v3p1-70b-instruct",
        "model_provider": "fireworks",
        "model_temperature": 1.0,
        "model_max_tokens": 16384
      },
      "env_vars": {
        "CONTRACT_ID": "devbot.near",
        "IPFS_API_KEY": "",
        "IPFS_API_SECRET": "",
        "GROUP_KEY": "",
        "NEAR_PRIVATE_KEY": "",
        "GROUP_ID": "theosis"
      }
    }
  },
  "show_entry": true
}