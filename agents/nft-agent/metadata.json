{
  "name": "nft-agent",
  "version": "0.0.2",
  "description": "Mints and transfers NFTs on 1000fans.testnet for 1000fans platform access.",
  "category": "agent",
  "tags": ["nft", "near", "token", "access"],
  "details": {
    "agent": {
      "defaults": {
        "model": "llama-v3p1-70b-instruct",
        "model_provider": "fireworks",
        "model_temperature": 1.0,
        "model_max_tokens": 16384
      },
      "env_vars": {
        "NFT_CONTRACT_ID": "1000fans.testnet",
        "GROUP_ID": "theosis",
        "ADMIN_PRIVATE_KEY": ""
      }
    }
  },
  "show_entry": true
}